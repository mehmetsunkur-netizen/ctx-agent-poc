{
  "name": "@isara-ctx/code-search-eval",
  "version": "0.1.0",
  "description": "Evaluation framework for code-search agent",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "code-search-eval": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist",
    "dev": "node --loader ts-node/esm src/cli.ts",
    "cli": "node dist/cli.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "agent",
    "evaluation",
    "code-search"
  ],
  "dependencies": {
    "@isara-ctx/code-agent": "workspace:*",
    "@isara-ctx/agent-framework": "workspace:*",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "meow": "^13.1.0",
    "openai": "^4.70.3",
    "zod": "^3.22.4",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
